    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "-c",
                "--data-path",
                "${workspaceFolder}/tests/",
                "--source-path",
                "${workspaceFolder}/tests/",
                "--graph-path",
                "${workspaceFolder}/tests/",
                "--output-file",
                "${workspaceFolder}/tests/branch_counts_capture.info"
            ],
            "console": "integratedTerminal"
        }
    ]
}